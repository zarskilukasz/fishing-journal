---
/**
 * Trip details page.
 * Displays full information about a specific fishing trip.
 */
import AppLayout from "@/layouts/AppLayout.astro";

const { id } = Astro.params;
---

<AppLayout title="Szczegóły wyprawy" showBackButton>
  <div class="space-y-4">
    <p class="text-muted-foreground">Szczegóły wyprawy {id} pojawią się tutaj.</p>
  </div>
</AppLayout>
